{
  "name": "My Project",
  "export": { "container": "png" },
  "compositions": [
    {
      "name": "My Composition",
      "width": 1920,
      "height": 1080,
      "background_color": { "r": 0, "g": 0, "b": 0, "a": 255 },
      "color_profile": "srgb",
      "fps": 30,
      "duration": 1,
      "tracks": [
        {
          "name": "My Track",
          "entities": [
            {
              "type": "image",
              "start_time": 0.0,
              "end_time": 1.0,
              "properties": {
                "file_path": { "type": "constant", "properties": { "value": "test_data/test.png" } },
                "position": { "type": "constant", "properties": { "value": { "x": 400, "y": 100 } } }
              },
              "effects": [
                {
                  "type": "blur",
                  "properties": { "blur_radius": { "type": "constant", "properties": { "value": 50.0 } } }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
